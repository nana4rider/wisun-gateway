{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${env:NVM_BIN}${env:NVM_SYMLINK}/npm",
      "args": ["run", "dev"],
      "name": "npm run dev",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ]
}
